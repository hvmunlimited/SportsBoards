<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"

	package="com.sportsboards" 
	android:versionCode="1" 
	android:versionName="1.0">
	<application android:icon="@drawable/sb_launcher"
		android:label="@string/app_name">

		<activity android:name="com.sportsboards.db.XMLParseTest"

			android:label="@string/app_name" android:screenOrientation="landscape">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		
		
		<activity android:name="com.sportsboards.launcher.LicenseCheck"
			android:screenOrientation="landscape" />
			
			
		<activity android:name="com.sportsboards.boards.SoccerBoard"
			android:screenOrientation="landscape" />
		<activity android:name="com.sportsboards.boards.BBallBoard"
			android:screenOrientation="landscape" />
			
		<activity android:name="com.sportsboards.settings.Launcher"
			android:screenOrientation="landscape" />	
			
		

	</application>

	<supports-screens android:largeScreens="true"></supports-screens>
	<uses-sdk android:targetSdkVersion="8" android:minSdkVersion="4"></uses-sdk>

	<uses-permission android:name="android.permission.WAKE_LOCK" />
	<uses-permission android:name="android.permission.CAMERA" />
	<uses-permission android:name="com.android.vending.CHECK_LICENSE">

	</uses-permission>

</manifest> 
